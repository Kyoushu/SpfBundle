{
  "name": "kyoushu/spf-bundle",
  "description": "A bundle for building pages ustilising SPF",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "James Willans",
      "email": "kyoushu@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/framework-bundle": ">=2.3",
    "symfony/twig-bundle": ">=2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "symfony/css-selector": ">=2.3",
    "symfony/dom-crawler": "~2.6",
    "symfony/browser-kit": "~2.3"
  },
  "autoload": {
    "psr-0": { "": "src" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
